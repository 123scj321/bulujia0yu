/* pages/product-detail/index.wxss */
page{
  background: #cef2f2;
}
.top{
  width:100%;
  height: 370rpx;
}
.top video{
  width:100%;
  height: 370rpx;
  position: relative;
}
.top video .play_video{
  padding:20rpx;
  width:100rpx;
  height: 100rpx;
  position: absolute;
  background:#333;
  left:50%;
  top:50%;
  margin-top:-90rpx;
