}
/* 面板 */
.switch-panel{
  width: 100%;
  height: 100%;
  position: relative;
  font-size: 32rpx;
  margin:10rpx auto;
}
.switch-panel-title-fixed{
  width: 750rpx;
  background: #cef2f2;
  position: fixed;
  text-align: center;
  top:0rpx;
  color: #555555;
  display: flex;
  border-bottom:2rpx solid #f2f2f2;
  padding-bottom:8rpx;
  z-index:1000;
  
}
.switch-panel-title-fixed .active text{
  color: #ffa84d;
}
.switch-panel-title-fixed  .active icon{
  display: block;
  margin:0 auto;
  width:80rpx;
  height: 8rpx;
  border-radius:20rpx;
  background: #ffa84d;
}
.switch-panel-title{
  margin:0rpx auto;
  width: 700rpx;
  background: #cef2f2;
  display: flex;
  height: 70rpx;
  line-height: 70rpx;
  border-bottom:2rpx solid #f2f2f2;
  padding-bottom:8rpx;
  font-weight: bold;
  font-size:32rpx;
  color:#555555;
}
.switch-panel-title .active text{
  color: #ffa84d;
}
.switch-panel-title .active icon{
  display: block;
  margin:0 auto;
  width:80rpx;
  height: 8rpx;
  border-radius:20rpx;
  background: #ffa84d;
}
.switch-panel-title-item{
  flex: 1;
  text-align: center;
}
.switch-panel-swiper{
  width: 100%;
  height: 1000rpx;
}
.switch-panel-content{
  width: 100%;
  height: 100%;
  padding-bottom: 100rpx;
}
/* 订单模块  */
.order-module{
  background: #cef2f2;
